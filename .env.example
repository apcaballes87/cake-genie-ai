# Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to Git!
#
# IMPORTANT: Next.js requires the NEXT_PUBLIC_ prefix for client-side variables.
# All environment variables that need to be accessed in the browser must use this prefix.

# =============================================================================
# REQUIRED - Core Services
# =============================================================================

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Role Key (server-side only - no NEXT_PUBLIC_ prefix needed)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Google Gemini API Key (required for AI cake analysis)
NEXT_PUBLIC_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# =============================================================================
# OPTIONAL - Enhanced Features
# =============================================================================

# Google Maps API Key (for location features)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Roboflow Configuration (for enhanced object detection)
NEXT_PUBLIC_ROBOFLOW_API_KEY=your_roboflow_api_key_here
NEXT_PUBLIC_ROBOFLOW_WORKSPACE=genieph
NEXT_PUBLIC_ROBOFLOW_WORKFLOW_ID=playground-florence-2-od

# Roboflow Feature Flags
NEXT_PUBLIC_USE_ROBOFLOW=true
NEXT_PUBLIC_ROBOFLOW_CONFIDENCE=0.3
NEXT_PUBLIC_ROBOFLOW_FALLBACK=true