{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://cqmhanqnfybyxezhobkx.supabase.co/functions/v1/generate-sitemap"
    },
    {
      "source": "/designs/:slug",
      "destination": "https://cqmhanqnfybyxezhobkx.supabase.co/functions/v1/share-design/:slug"
    },
    {
      "source": "/share/:id*",
      "destination": "https://cqmhanqnfybyxezhobkx.supabase.co/functions/v1/share-design/:id*"
    }
  ],
  "headers": [
    {
      "source": "/designs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'unsafe-inline'; style-src 'unsafe-inline';"
        }
      ]
    },
    {
      "source": "/share/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'unsafe-inline'; style-src 'unsafe-inline';"
        }
      ]
    }
  ]
}