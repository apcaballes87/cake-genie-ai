{
  "rewrites": [
    {
      "source": "/designs/:slug*",
      "destination": "https://cqmhanqnfybyxezhobkx.supabase.co/functions/v1/share-design/:slug*"
    },
    {
      "source": "/share/:id*",
      "destination": "https://cqmhanqnfybyxezhobkx.supabase.co/functions/v1/share-design/:id*"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://cqmhanqnfybyxezhobkx.supabase.co/functions/v1/sitemap"
    }
  ],
  "headers": [
    {
      "source": "/designs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/share/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}