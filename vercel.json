{
  "rewrites": [],
  "headers": [
    {
      "source": "/designs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'unsafe-inline'; style-src 'unsafe-inline';"
        }
      ]
    },
    {
      "source": "/share/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'unsafe-inline'; style-src 'unsafe-inline';"
        }
      ]
    }
  ]
}