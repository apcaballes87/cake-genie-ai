{
  "redirects": [
    {
      "source": "/designs/:slug",
      "destination": "/#/designs/:slug",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/share/:id*",
      "destination": "https://cqmhanqnfybyxezhobkx.supabase.co/functions/v1/share-design/:id*"
    }
  ],
  "headers": [
    {
      "source": "/designs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/share/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}